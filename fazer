    Tab3:Button("Force Reset",function()
        game.Workspace.Camera.CameraType = "Scriptable"
        for i,v in next, game:GetService('Players').LocalPlayer.Character:GetChildren() do
        if v:IsA('Accessory') then
           v:Destroy()
        end
        end
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-20.6895447, -378.734528, -1302.26074, -0.972757995, -0.172171488, -0.155237958, -2.70296763e-08, 0.669640481, -0.742685437, 0.231822878, -0.722453177, -0.651398122)
