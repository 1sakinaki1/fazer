    end)

	Tab3:Button("Invinsible",function()
        game:GetService("Workspace").Camera.CameraType = "Fixed"
        local Hum = game.Players.LocalPlayer.Character.HumanoidRootPart
        local PlayerPosition = { Hum.CFrame:GetComponents() } -- gets your current pos (oldPosition)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new (22.5130901, -58.7000732, 143.723831, -0.0375774987, 4.70444306e-09, 0.999293745, -1.36600647e-10, 1, -4.71290473e-09, -0.999293745, -3.13603338e-10, -0.0375774987)
        wait(0.1)
        game.Players.LocalPlayer.Character.LowerTorso:remove()
        Hum.CFrame = CFrame.new(unpack(PlayerPosition)) -- teleports you to oldPosition
        game:GetService("Workspace").Camera.CameraType = "Custom"
