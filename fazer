			Tab4:Toggle("Fling",false,function(state)
				if state then

                     
                     -- Objects
                     local Players = game:GetService("Players")
                     local RunService = game:GetService("RunService")
                     
                     local LocalPlayer = Players.LocalPlayer
                     local Target = Players[plr]
                     
                     local BodyAngularVelocity = Instance.new("BodyAngularVelocity")
                     BodyAngularVelocity.AngularVelocity = Vector3.new(11^6, 11^6, 11^6)
                     BodyAngularVelocity.MaxTorque = Vector3.new(11^6, 11^6, 11^6)
                     BodyAngularVelocity.P = 11^6
                     
                     -- Start
                
                     BodyAngularVelocity.Parent = LocalPlayer.Character.HumanoidRootPart
                     
                     while game.Players[plr].Character.HumanoidRootPart and LocalPlayer.Character.HumanoidRootPart do
                        RunService.RenderStepped:Wait()
                        LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[plr].Character.HumanoidRootPart.CFrame * LocalPlayer.Character.HumanoidRootPart.CFrame.Rotation
                        LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new()
                     end
                        
                     BodyAngularVelocity.Parent = nil
				else
                    game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
					game.Players.LocalPlayer.Character.Humanoid:remove()    
				end
				end)


